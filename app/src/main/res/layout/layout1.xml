<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="280dp"
        android:id="@+id/relm"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="5dp">

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="5dp"
            android:background="#cac7c7" />


        <RelativeLayout
            android:id="@+id/re1"
            android:layout_width="match_parent"
            android:layout_height="340dp"
            android:layout_centerHorizontal="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@id/relative">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image1"
                    android:layout_width="76dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/doctors2"/>

                <TextView
                    android:text="Jordan Griffin"
                    android:textStyle="bold"
                    android:textColor="#000"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/textView1" />
            </RelativeLayout>
            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_marginLeft="90dp"
                android:layout_below="@+id/relative1"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="#cac7c7" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@+id/relative2"
                android:layout_below="@+id/relative1">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image2"
                    android:layout_width="76dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/doctor1"/>
                <TextView
                    android:text="Aaron Gutierrez"
                    android:textStyle="bold"
                    android:textColor="#000"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/textView2" />
            </RelativeLayout>
            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_marginLeft="90dp"
                android:layout_below="@+id/relative2"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="#cac7c7" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@+id/relative3"
                android:layout_below="@+id/relative2">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image3"
                    android:layout_width="76dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/doctors2"/>
                <TextView
                    android:text="Robert Bowman"
                    android:textStyle="bold"
                    android:textColor="#000"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/textView3" />

            </RelativeLayout>
            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_marginLeft="90dp"
                android:layout_below="@+id/relative3"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="#cac7c7" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:id="@+id/relative4"
                android:layout_below="@+id/relative3">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image4"
                    android:layout_width="76dp"
                    android:layout_height="60dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="0dp"
                    android:src="@drawable/doctor1"/>
                <TextView
                    android:text="DR.Dylan Cole"
                    android:textStyle="bold"
                    android:textColor="#000"
                    android:layout_marginLeft="90dp"
                    android:layout_marginTop="30dp"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:id="@+id/textView4" />
            </RelativeLayout>
            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_below="@+id/relative4"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="#cac7c7" />

        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
